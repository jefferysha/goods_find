# 生产环境配置
ENV_STATE=prod
DEBUG=False

# 项目信息
PROJECT_NAME=GoodFish System
API_V1_STR=/api/v1

# 数据库配置
DATABASE_URL=postgresql+asyncpg://prod_user:prod_password@db-prod.example.com:5432/goods_find_prod

# 安全配置
SECRET_KEY=prod-secret-key-must-be-changed
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# 爬虫配置
SCRAPER_HEADLESS=True
SCRAPER_MAX_WORKERS=5
SCRAPER_TIMEOUT=30
SCRAPER_USER_DATA_DIR=./user_data

# 代理配置
PROXY_ENABLED=True
PROXY_CHECK_INTERVAL=300

# 反爬配置
MIN_REQUEST_INTERVAL=3.0
MAX_REQUEST_INTERVAL=8.0
